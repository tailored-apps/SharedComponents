<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<SourceRevisionId>build$([System.DateTime]::UtcNow.ToString("yyyyMMddHHmmss"))</SourceRevisionId>
		<TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
		<AssemblyName>TailoredApps.Shared.DateTime</AssemblyName>
		<RootNamespace>TailoredApps.Shared.DateTime</RootNamespace>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>0.0.1</Version>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>

		<Authors>Lukasz Kowalski</Authors>
		<Company>Tailored Apps.</Company>
		<Product>Shared.DateTime</Product>
		<Description>Shared.DateTime is a simple abstraction for DateTime for .NET</Description>
		<Copyright>Copyright Â© Lukasz Kowalski</Copyright>
		<Summary>Shared.DateTime is a simple abstraction for DateTime for .NET</Summary>
		<NeutralLanguage>en-US</NeutralLanguage>
		<Title>Shared.DateTime</Title>
		<PackageId>TailoredApps.Shared.DateTime</PackageId>
		<PackageTags>Mediator support for .NET</PackageTags>
		<PackageIconUrl>https://www.nuget.org/profiles/TailoredApps/avatar?imageSize=512</PackageIconUrl>
		<PackageProjectUrl>https://shared.tailoredapps.pl</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
	</PropertyGroup>

</Project>
